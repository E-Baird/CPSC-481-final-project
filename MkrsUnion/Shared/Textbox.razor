<form>
	<div class="row mb-3">
		<label for=textbox class="col-sm-2 col-form-label">@label</label>
		<div class="col-sm-10">
			<input type="text" class="form-control" value="@data" />
		</div>
	</div>
</form>

@code {
	[Parameter]
	public string data { get; set; }

	[Parameter]
    public string? label { get; set; }
}
